<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wttg">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature
        android:glEsVersion="0x00020000"        android:required="true" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_icon1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_icon1_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ManagerEditProfileActivity"></activity>
        <activity android:name=".UserEditProfileActivity" />
        <activity android:name=".CameraActivity" />
        <activity android:name=".UserHomeActivity" />
        <activity android:name=".ManagerHomeActivity" />
        <activity android:name=".UserRegisterActivity" />
        <activity android:name=".ManagerRegisterActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>